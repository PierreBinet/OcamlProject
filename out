digraph finite_state_machine {
 node [shape = circle];
4 -> 5 [ label = "0 /4" ];
4 -> 3 [ label = "0 /7" ];
3 -> 5 [ label = "0 /20" ];
3 -> 2 [ label = "0 /9" ];
2 -> 4 [ label = "0 /14" ];
2 -> 1 [ label = "0 /4" ];
1 -> 3 [ label = "0 /12" ];
1 -> 2 [ label = "0 /10" ];
0 -> 2 [ label = "0 /13" ];
0 -> 1 [ label = "0 /16" ];
}